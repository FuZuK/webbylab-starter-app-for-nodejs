{
  "name": "modern-node-be",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "nodemon": "MODE=application nodemon app.mjs",
    "nodemon:docker": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "start": "MODE=application NODE_ENV=production node app.mjs",
    "test:lint": "eslint lib tests app.mjs --ext mjs",
    "test:ava": "cross-env MODE=test ava",
    "test:api": "cross-env MODE=test ava tests/api/**/*.test.mjs",
    "test:use-cases": "cross-env MODE=test ava tests/use-cases/**/*.test.mjs",
    "test:coverage": "c8 npm run test:ava",
    "test:audit": "npm audit",
    "test": "npm run test:lint && npm run test:audit && npm run test:ava && npm run test:coverage",
    "create:admin": "node ./bin/add_admin.mjs",
    "migration:test": "sequelize db:migrate --config lib/config.cjs --env test-db",
    "migration:db": "sequelize db:migrate --config lib/config.cjs --env db"
  },
  "pre-commit": [
    "test:lint"
  ],
  "dependencies": {
    "aws-sdk": "^2.645.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "busboy": "^0.3.1",
    "chista": "^1.0.0",
    "cls-hooked": "^4.2.2",
    "confme": "0.0.14",
    "connect-busboy": "0.0.2",
    "cors": "^2.8.5",
    "docopt": "^0.6.2",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.6",
    "json-pointer": "^0.6.0",
    "jsonwebtoken": "^8.4.0",
    "livr": "^2.3.0",
    "livr-extra-rules": "^1.1.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.6",
    "nodemailer-sendmail-transport": "^1.0.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "pino": "^6.0.0",
    "rename-keys": "^2.0.1",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "ava": "^3.5.1",
    "babel-eslint": "^10.1.0",
    "c8": "^7.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-webbylab": "^4.0.0",
    "eslint-plugin-ava": "^10.2.0",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-more": "0.3.2",
    "eslint-plugin-no-require-lodash": "^1.1.0",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "eslint-plugin-react": "^7.19.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "nodemailer-mock": "^1.5.1",
    "nodemailer-stub-transport": "^1.1.0",
    "pino-pretty": "^3.6.1",
    "pre-commit": "^1.2.2"
  },
  "description": "NodeJS Backend Starterkit",
  "main": "app.mjs",
  "repository": {
    "type": "git",
    "url": "http://gitlab.webbylab.com/boilerplates/modern-node-be"
  },
  "author": "WebbyLab"
}
