{
  "name": "modern-node-be",
  "type": "module",
  "config": {
    "MOCHA_OPTS": "--timeout 60000 --exit --reporter spec --ui qunit tests/api/**/*.js",
    "ISPARTA_OPTS": "--report html",
    "COVERAGE_OPTS": "--statements 85 --functions 85 --branches 85 --lines 85"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "nodemon": "MODE=application nodemon app.js",
    "nodemon:docker": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "start": "MODE=application NODE_ENV=production node app.js",
    "test:lint": "eslint lib tests app.js --ext js",
    "test:mocha": "cross-env MODE=test node node_modules/.bin/_mocha $npm_package_config_MOCHA_OPTS",
    "test:coverage": "npm run coverage && npm run coverage-check",
    "test:audit": "npm audit",
    "test": "npm run test:lint && npm run test:audit && npm run test:mocha && npm run test:coverage",
    "coverage": "cross-env MODE=test node -r node_modules/istanbul/lib/cli.js cover $npm_package_config_ISPARTA_OPTS node_modules/.bin/_mocha -- $npm_package_config_MOCHA_OPTS",
    "coverage-check": "cross-env MODE=test node_modules/istanbul/lib/cli.js check-coverage $npm_package_config_COVERAGE_OPTS",
    "create-admin": "node ./bin/add_admin.js",
    "migration:test": "sequelize db:migrate --config etc/config.cjs --env test-db",
    "migration:db": "sequelize db:migrate --config etc/config.cjs --env db"
  },
  "pre-commit": [
    "test:lint"
  ],
  "dependencies": {
    "aws-sdk": "^2.642.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "busboy": "^0.3.1",
    "chista": "^1.0.0",
    "cls-hooked": "^4.2.2",
    "confme": "0.0.12",
    "connect-busboy": "0.0.2",
    "cors": "^2.8.5",
    "docopt": "^0.6.2",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.7.3",
    "json-pointer": "^0.6.0",
    "jsonwebtoken": "^8.4.0",
    "livr": "^2.3.0",
    "livr-extra-rules": "^1.1.0",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.4",
    "nodemailer-sendmail-transport": "^1.0.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemailer-stub-transport": "^1.1.0",
    "rename-keys": "^2.0.1",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "uuid": "^7.0.1",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-webbylab": "^4.0.0",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-more": "0.3.2",
    "eslint-plugin-no-require-lodash": "^1.1.0",
    "eslint-plugin-prefer-spread": "^1.0.3",
    "eslint-plugin-react": "^7.18.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "pre-commit": "^1.2.2"
  },
  "description": "NodeJS Backend Starterkit",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "http://gitlab.webbylab.com/boilerplates/modern-node-be"
  },
  "author": "WebbyLab"
}
