{
    "mainUrl": "{{MAIN_URL}}",
    "staticUrl": "{{STATIC_URL}}",
    "s3": {
        "accessKeyId": "{{S3_ACCESS_KEY_ID}}",
        "secretAccessKey": "{{S3_SECRET_ACCESS_KEY}}",
        "bucket": "{{S3_BUCKET}}"
    },
    "secret":  "{{SECRET}}",
    "appPort": "{{PORT}}",
    "testStaticPath": "./test_static",
    "postImagesMaxSize": 4000000,
    "imagesPath"       : "images/",
    "mail": {
        "from": "{{MAIL_FROM}}",
        "transport": "{{MAIL_TRANSPORT}}",
        "transport_options": {
            "host" : "{{MAIL_HOST}}",
            "port" : "{{MAIL_PORT}}",
            "auth": {
                "user": "{{MAIL_AUTH_USER}}",
                "pass": "{{MAIL_AUTH_PASS}}",
                "region": "us-east-1"
            }
        }
    }
}